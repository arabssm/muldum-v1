{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "baseUrl": ".",
    "paths": {
      "@_assets/*": ["src/assets/*"],
      "@_modalImages/*": ["src/assets/modal/*"],
      "@_components/*": ["src/admin/components/*"],
      "@_menu/*": ["src/admin/components/Menu/*"],
      "@_modal/*": ["src/admin/components/Modal/*"],
      "@_navbar/*": ["src/all/component/sibebar/*"],
      "@_notfound/*": ["src/admin/components/NotFound/*"],
      "@_pages/*": ["src/admin/page/*"],
      "@_main/*": ["src/admin/page/Main/*"],
      "@_notice/*": ["src/all/component/Notice/*"],
      "@_styles/*": ["src/App.css"],
      "@_page/*": ["src/student/page/*"],
      "@_component/*": ["src/student/component/*"],
      "@_all/*": ["src/all/*"],
      "@_api/*": ["src/api/*"],
      "@_util/*": ["src/student/util/*"]
    },
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true
  },
  "include": ["src"], 
  "exclude": ["node_modules"]
}